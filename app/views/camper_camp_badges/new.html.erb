<div class="header" id="profile-header">
  <%#= link_to 'Back', :back %>
  <div class="btn-wrapper">
  </div>
  <div class="header-body">
    <div class="header-title">Adding New Camper to Camp Badge</div>
    <div class="header-subtitle">
      <%= @camp_badge.camp.name %>
    </div>
  </div>
</div>

<div class="row page-row">
  <div class="col">
    <div class="card page-card">
      <div class="card-body">
        <h5 class="card-title">Add a Camper to a Camp Badge</h5>
        <p class="card-text">
          <%= render 'form', camper_camp_badge: @camper_camp_badge %>
        </p>
      </div>
    </div>
  </div>
</div>

<div class="pad-bottom">
</div>

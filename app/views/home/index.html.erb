<% if logged_in? && (current_user.role?(:admin)) %>
  <%= render :partial => "admin_dashboard" %>
<% elsif logged_in? && (current_user.role?(:parent)) %>
  <%= render :partial => "parent_dashboard" %>
<% elsif logged_in? && (current_user.role?(:counselor)) %>
  <%= render :partial => "counselor_dashboard" %>
<% else %>
  <%= render :partial => "index_guest" %>
<% end %>